{
  "version": "1.2.0-*",
  "title": "ASP.NET SimpleIdentity",
  "description": "Simple ASP.NET Core authentication and identity provider. Valid users are specified in a simple configuration file. Please refer to the project site at http://dan.cx/projects/SimpleIdentity for full installation instructions, usage examples, and sample code.",
  "authors": [ "Daniel Lo Nigro" ],

  "dependencies": {
    "Microsoft.AspNetCore.Identity": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0"
  },

  "frameworks": {
    "net451": { },
    "netstandard1.3": {
      "dependencies": {
        "System.Linq.Queryable": "4.0.1"
      }
    }
  },

  "packOptions": {
    "tags": [ "authentication", "authorization", "identity", "configuration" ],
    "projectUrl": "http://dan.cx/projects/simpleidentity",
    "licenseUrl": "https://github.com/Daniel15/SimpleIdentity/blob/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "git://github.com/Daniel15/SimpleIdentity"
    }
  }
}
